{
  "expo": {
    "name": "Fun Planner",
    "slug": "FunPlanner",
    "sdkVersion": "37.0.3",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.9.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shayanys.funplanner"
    },
    "android": {
      "package": "com.shayanys.funplanner"
    }
  }
}
